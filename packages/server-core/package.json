{
	"name": "@wwwallet-private/server-core",
	"version": "0.1.0-alpha.3",
	"license": "BSD-2-Clause",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"import": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"require": {
			"types": "./dist/index.d.cts",
			"require": "./dist/index.cjs"
		}
	},
	"scripts": {
		"build": "tsup-node",
		"dev": "tsup-node --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:cover": "vitest run --coverage",
		"dependency-tree": "madge --image ../../images/packages-server-core-dependency-tree.svg ./src/index.ts"
	},
	"devDependencies": {
		"@types/supertest": "^6.0.3",
		"madge": "^8.0.0",
		"supertest": "^7.1.4",
		"tsup": "^8.5.0",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@sd-jwt/core": "^0.14.1",
		"@sd-jwt/crypto-nodejs": "^0.14.0",
		"@sd-jwt/utils": "^0.14.0",
		"@types/express": "^5.0.3",
		"@types/qrcode": "^1.5.5",
		"ajv": "^8.17.1",
		"express": "^5.1.0",
		"jose": "^6.1.0",
		"qrcode": "^1.5.4",
		"ts-deepmerge": "^7.0.3"
	}
}
