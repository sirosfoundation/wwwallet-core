		"jsdom": "^26.1.0",
		"madge": "^8.0.0",
		"react": "^19.1.1",
		"supertest": "^7.1.4",
		"tsup": "^8.5.0",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@types/web": "^0.0.265",
		"ajv": "^8.17.1",
		"axios": "^1.11.0",
