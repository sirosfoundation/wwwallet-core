{
	"scripts": {
		"prepare": "husky",
		"start": "ts-node --typeCheck --transpileOnly --project ./tsconfig.json ./main.ts",
		"config:validate": "ts-node --typeCheck --transpileOnly --project ./tsconfig.json ./main.validateConfig.ts",
		"format": "biome format --write",
		"biome:ci": "biome ci",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:cover": "vitest run --coverage",
		"dependency-tree": "madge --image ./images/dependency-tree.svg ./main.ts && open ./images/dependency-tree.svg"
	},
	"devDependencies": {
		"@biomejs/biome": "2.1.2",
		"@tsconfig/node22": "^22.0.2",
		"@types/qrcode": "^1.5.5",
		"@types/supertest": "^6.0.3",
		"@vitest/coverage-v8": "^3.2.4",
		"husky": "^9.1.7",
		"madge": "^8.0.0",
		"supertest": "^7.1.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.8.3",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@sd-jwt/core": "^0.14.1",
		"@sd-jwt/crypto-nodejs": "^0.14.0",
		"@sd-jwt/utils": "^0.14.0",
		"@types/express": "^5.0.3",
		"@types/morgan": "^1.9.10",
		"@types/yaml": "^1.9.6",
		"ajv": "^8.17.1",
		"express": "^5.1.0",
		"express-handlebars": "^8.0.3",
		"express-list-routes": "^1.3.1",
		"handlebars": "^4.7.8",
		"inversify": "^7.6.1",
		"jose": "^6.0.12",
		"morgan": "^1.10.1",
		"qrcode": "^1.5.4",
		"ts-deepmerge": "^7.0.3",
		"yaml": "^2.8.1"
	},
	"packageManager": "pnpm@10.4.0"
}
